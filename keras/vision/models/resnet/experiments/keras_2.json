{
    "data": {
        "module": "data_loaders.cifar10.data_loader",
        "function": "get_data_loader",
        "object_key": "data",
        "params": {
            "batch_size": 128
        }
    },
    "trainer": {
        "module": "modules.resnet.resnet",
        "function": "get_model",
        "params": {
            "input_shape": [
                180,
                180,
                3
            ],
            "num_classes": 2
        },
        "object_key": "model",
        "method_args": [
            {
                "function": "load_weights",
                "params": {
                    "filepath": "resnet50_weights_tf_dim_ordering_tf_kernels_notop.h5"
                }
            },
            {
                "function": "compile",
                "params": {
                    "optimizer": {
                        "module": "tensorflow.keras.optimizers",
                        "class": "Adam",
                        "params": {
                            "learning_rate": 0.001
                        }
                    },
                    "loss": "binary_crossentropy",
                    "metrics": [
                        "accuracy"
                    ]
                }
            },
            {
                "function": "fit",
                "params": {
                    "": {
                        "__object": true,
                        "__refrence_key": "data"
                    },
                    "batch_size": 32,
                    "epochs": 10,
                    "validation_split": 0.2
                }
            }
        ]
    }
}